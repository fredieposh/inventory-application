            <div class="table-container">
                <table>
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Product</th>
                            <th>Category</th>
                            <th>Price</th>
                            <th>Quantity</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% rows.forEach(row => { %>
                            <tr>
                                <td><%= row.id %></td>
                                <td><%= row.product_name %></td>
                                <td><%= row.category %></td>
                                <td><%= row.price %> $</td>
                                <td><%= row.quantity %></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>